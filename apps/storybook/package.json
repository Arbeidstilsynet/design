{
  "name": "@arbeidstilsynet/storybook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter @arbeidstilsynet/design-css dev\" \"pnpm dev:only\"",
    "dev:only": "storybook dev -p 6006",
    "build": "storybook build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "serve": "pnpm dlx http-server ./storybook-static",
    "circular": "pnpm dlx madge --circular --extensions \"js,jsx,ts,tsx\" ./stories"
  },
  "dependencies": {
    "@arbeidstilsynet/design-css": "workspace:*",
    "@arbeidstilsynet/design-theme": "workspace:*",
    "@navikt/aksel-icons": "7.28.1",
    "clsx": "2.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@storybook/addon-docs": "9.1.3",
    "@storybook/addon-storysource": "8.6.14",
    "@storybook/react-vite": "9.1.3",
    "@types/node": "22.18.0",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.7",
    "concurrently": "9.2.1",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc1",
    "eslint-plugin-storybook": "9.1.3",
    "storybook": "9.1.3",
    "typescript": "5.9.2",
    "typescript-eslint": "8.41.0"
  }
}
