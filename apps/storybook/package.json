{
  "name": "@arbeidstilsynet/storybook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter @arbeidstilsynet/design-css dev\" \"pnpm dev:only\"",
    "dev:only": "storybook dev -p 6006",
    "build": "storybook build",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "serve": "pnpm dlx http-server ./storybook-static",
    "circular": "pnpm dlx madge --circular --extensions \"js,jsx,ts,tsx\" ./stories"
  },
  "dependencies": {
    "@arbeidstilsynet/design-css": "workspace:*",
    "@arbeidstilsynet/design-react": "workspace:*",
    "@arbeidstilsynet/design-theme": "workspace:*",
    "@navikt/aksel-icons": "8.4.0",
    "clsx": "2.1.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@storybook/addon-a11y": "10.2.8",
    "@storybook/addon-docs": "10.2.8",
    "@storybook/addon-vitest": "10.2.8",
    "@storybook/react-vite": "10.2.8",
    "@testing-library/dom": "10.4.1",
    "@types/node": "24.10.13",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "concurrently": "9.2.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-storybook": "10.2.8",
    "playwright": "1.58.2",
    "storybook": "10.2.8",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "vitest": "4.0.18"
  }
}
