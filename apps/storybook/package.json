{
  "name": "@arbeidstilsynet/storybook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter @arbeidstilsynet/design-css dev\" \"pnpm dev:only\"",
    "dev:only": "storybook dev -p 6006",
    "build": "storybook build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "serve": "pnpm dlx http-server ./storybook-static",
    "circular": "pnpm dlx madge --circular --extensions \"js,jsx,ts,tsx\" ./stories"
  },
  "dependencies": {
    "@arbeidstilsynet/design-css": "workspace:*",
    "@arbeidstilsynet/design-theme": "workspace:*",
    "@navikt/aksel-icons": "7.24.0",
    "clsx": "2.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@storybook/addon-docs": "9.0.16",
    "@storybook/addon-storysource": "8.6.14",
    "@storybook/react-vite": "9.0.16",
    "@types/node": "22.15.35",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "concurrently": "9.2.0",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc1",
    "eslint-plugin-storybook": "9.0.16",
    "storybook": "9.0.16",
    "typescript": "5.8.3",
    "typescript-eslint": "8.37.0"
  }
}
