{
  "name": "@arbeidstilsynet/storybook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter @arbeidstilsynet/design-css dev\" \"pnpm dev:only\"",
    "dev:only": "storybook dev -p 6006",
    "build": "storybook build",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "serve": "pnpm dlx http-server ./storybook-static",
    "circular": "pnpm dlx madge --circular --extensions \"js,jsx,ts,tsx\" ./stories"
  },
  "dependencies": {
    "@arbeidstilsynet/design-css": "workspace:*",
    "@arbeidstilsynet/design-theme": "workspace:*",
    "@navikt/aksel-icons": "7.32.3",
    "clsx": "2.1.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@storybook/addon-a11y": "9.1.16",
    "@storybook/addon-docs": "9.1.16",
    "@storybook/addon-storysource": "8.6.14",
    "@storybook/addon-vitest": "9.1.16",
    "@storybook/react-vite": "9.1.16",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "concurrently": "9.2.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-storybook": "9.1.16",
    "playwright": "1.56.1",
    "storybook": "9.1.16",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  }
}
